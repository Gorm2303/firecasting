{
  "title": "Spring Boot Overview",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Requests /s",
      "targets": [
        { "expr": "rate(http_server_requests_seconds_count[1m])", "legendFormat": "{{uri}} {{status}}" }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "HTTP Latency p95 (s)",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[5m])) by (le))" }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 }
    },
    {
      "type": "timeseries",
      "title": "JVM Memory Used",
      "targets": [
        { "expr": "jvm_memory_used_bytes{area=\"heap\"}", "legendFormat": "heap" },
        { "expr": "jvm_memory_used_bytes{area=\"nonheap\"}", "legendFormat": "nonheap" }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" } },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 }
    }
  ],
  "time": { "from": "now-6h", "to": "now" }
}
