{
  "annotations": {
    "list": [
      {
        "type": "dashboard",
        "name": "Deploys (Watchtower)",
        "enable": true,
        "hide": false,
        "iconColor": "rgba(255, 96, 96, 1)",
        "datasource": { "type": "loki", "uid": "LOKI" },
        "expr": "{container=~\".*watchtower.*\"} |~ \"Found new|Stopping|Starting\"",
        "textFormat": "Deploy",
        "useValueForTime": false
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests /s by HTTP status",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PROM" },
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "targets": [
        { "refId": "A", "expr": "sum by (status)(rate(http_server_requests_seconds_count[1m]))", "legendFormat": "{{status}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP latency p95 (s)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PROM" },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "targets": [
        { "refId": "A", "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[5m])) by (le))", "legendFormat": "p95" }
      ]
    },
    {
      "type": "timeseries",
      "title": "5xx errors /s",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PROM" },
      "fieldConfig": { "defaults": { "unit": "req/s" } },
      "targets": [
        { "refId": "A", "expr": "sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[5m]))", "legendFormat": "5xx" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Errors /s by container (from logs)",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "loki", "uid": "LOKI" },
      "fieldConfig": { "defaults": { "unit": "req/s" } },
      "targets": [
        { "refId": "A", "expr": "sum by (container)( rate({job=\"docker\"} |= \"ERROR\" [5m]) )", "legendFormat": "{{container}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Container CPU (cores) by container",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "datasource": { "type": "prometheus", "uid": "PROM" },
      "fieldConfig": { "defaults": { "unit": "cores" } },
      "targets": [
        { "refId": "A", "expr": "sum by (name)( rate(container_cpu_usage_seconds_total{image!=\"\"}[2m]) )", "legendFormat": "{{name}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Container memory (bytes) by container",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "datasource": { "type": "prometheus", "uid": "PROM" },
      "fieldConfig": { "defaults": { "unit": "bytes" } },
      "targets": [
        { "refId": "A", "expr": "container_memory_usage_bytes{image!=\"\"}", "legendFormat": "{{name}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Traefik router requests /s (if metrics enabled)",
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 23 },
      "datasource": { "type": "prometheus", "uid": "PROM" },
      "fieldConfig": { "defaults": { "unit": "req/s" } },
      "targets": [
        { "refId": "A", "expr": "sum by (router)( rate(traefik_router_requests_total[1m]) )", "legendFormat": "{{router}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Log throughput (lines/s) by container",
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 30 },
      "datasource": { "type": "loki", "uid": "LOKI" },
      "fieldConfig": { "defaults": { "unit": "req/s" } },
      "targets": [
        { "refId": "A", "expr": "sum by (container)( rate({job=\"docker\"}[1m]) )", "legendFormat": "{{container}}" }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["firecasting", "ops"],
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s","10s","30s","1m","5m"] },
  "title": "Firecasting â€“ Ops Overview",
  "version": 1
}
